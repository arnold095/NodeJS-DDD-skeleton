{
  "name": "nodejs-ddd-skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc && tsc-alias -p tsconfig.json && npm run build:di",
    "build:clean": "rm -rf dist/*",
    "build:tsc": "tsc --build tsconfig.json",
    "build:di": "copyfiles 'src/**/*.{json,yaml,html,png}' dist",
    "test:all": "npm run test && npm run test:acceptance",
    "test": "jest -c jest.config.js --coverage",
    "test:unit": "jest -c jest.config.unit.js",
    "test:integration": "jest -c jest.config.integration.js",
    "test:acceptance": "cucumber-js -p auth --publish-quiet --exit",
    "test:watch": "jest -c jest.config.unit.js  --watch",
    "test:i-watch": "jest -c jest.config.integration.js  --watch",
    "start:dev": "nodemon --exec ts-node -T -r tsconfig-paths/register src/Apps/MyApp/App.ts -e ts",
    "auth:dev": "nodemon --exec ts-node -T -r tsconfig-paths/register src/Apps/Auth/App.ts -e ts",
    "consumer:start": "nodemon --exec ts-node -T -r tsconfig-paths/register src/Apps/Notifications/Command/RabbitMQ/ConsumeRabbitMQDomainEventsCommand.ts -e ts",
    "lint": "eslint '**/src/**/*.ts' --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnold095/NodeJS-DDD-skeleton.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arnold095/NodeJS-DDD-skeleton/issues"
  },
  "homepage": "https://github.com/arnold095/NodeJS-DDD-skeleton#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.0",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.8",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.13.3",
    "@types/koa__cors": "^3.0.2",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-router": "^7.4.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.9.6",
    "@types/nodemailer": "^6.4.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
